{
    "name": "@elizaos/plugin-bnb",
    "version": "0.1.9",
    "main": "dist/index.js",
    "type": "module",
    "types": "dist/index.d.ts",
    "dependencies": {
        "@bnb-chain/greenfield-cosmos-types": "^0.4.0",
        "@bnb-chain/greenfield-js-sdk": "^2.2.1",
        "@elizaos/core": "workspace:*",
        "@lifi/data-types": "5.15.5",
        "@lifi/sdk": "3.4.1",
        "@lifi/types": "16.3.0",
        "@openzeppelin/contracts": "^5.1.0",
        "@solana/web3.js": "^1.98.0",
        "@types/node": "^22.10.5",
        "@web3-name-sdk/core": "^0.3.2",
        "chalk": "^5.3.0",
        "cli-table3": "^0.6.3",
        "mime-types": "^2.1.35",
        "ora": "^8.0.1",
        "solc": "^0.8.28",
        "tsup": "8.3.5",
        "viem": "2.21.53",
        "ws": "^8.18.0"
    },
    "devDependencies": {
        "@biomejs/biome": "1.9.4"
    },
    "scripts": {
        "build": "tsup --format esm --dts",
        "dev": "tsup --format esm --dts --watch",
        "test": "vitest run",
        "clean": "rm -rf dist",
        "lint": "biome lint .",
        "lint:fix": "biome check --apply .",
        "format": "biome format .",
        "format:fix": "biome format --write ."
    },
    "peerDependencies": {
        "@types/mime-types": "^2.1.4",
        "whatwg-url": "7.1.0"
    },
    "agentConfig": {
        "pluginType": "elizaos:client:1.0.0",
        "pluginParameters": {
            "BNB_PRIVATE_KEY": {
                "type": "string",
                "minLength": 1,
                "description": "Your private key for the BNB Chain ecosystem. Grants full access to associated funds.",
                "optional": true
            },
            "BNB_PUBLIC_KEY": {
                "type": "string",
                "minLength": 1,
                "description": "Your public key for the BNB Chain ecosystem.",
                "optional": true
            },
            "BSC_PROVIDER_URL": {
                "type": "string",
                "minLength": 1,
                "description": "Custom RPC URL for BSC.",
                "optional": true
            },
            "OPBNB_PROVIDER_URL": {
                "type": "string",
                "minLength": 1,
                "description": "Custom RPC URL for opBNB.",
                "optional": true
            }
        }
    }
}